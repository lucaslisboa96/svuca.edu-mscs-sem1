WARN	:= -Wall -Wshadow -Os -fomit-frame-pointerINCLUDE	:= -I${INCPREFIX}/include -I.CFLAGS	:= ${WARN} -D_GNU_SOURCE ${INCLUDE}LDFLAGS	:= -s -Wl,-warn-common --staticCC	:= ${PREFIX}/bin/${TARGET}-gccSTRIP	:= ${PREFIX}/bin/${TARGET}-stripSTRIPCMD:= $(STRIP) -s --remove-section=.note --remove-section=.commentOBJS	:= ${patsubst %.c, %.o, ${wildcard *.c}}app:${OBJS}	$(CC) $(LDFLAGS) -o $@ $(OBJS)	$(STRIPCMD) $@..PHONY: cleanclean:	- rm -rf ${OBJS}